<mvc:View controllerName="com.pe.pm.curd.pm_curd.controller.Detail" 
			xmlns="sap.m" 
			xmlns:core="sap.ui.core" 
			xmlns:f="sap.ui.layout.form" 
			xmlns:l="sap.ui.layout" 
			xmlns:mvc="sap.ui.core.mvc" 
			xmlns:smart="sap.ui.comp.smartfield" 
			xmlns:semantic="sap.m.semantic" 
			xmlns:footerbar="sap.ushell.ui.footerbar">
	<semantic:DetailPage id="page" navButtonPress="onNavBack" showNavButton="{device>/system/phone}" title="{i18n>detailTitle}"
		busy="{detailView>/busy}" busyIndicatorDelay="{detailView>/delay}">
		<semantic:content>
			<!--
			<ObjectHeader id="objectHeader" title="{Qmnum}"></ObjectHeader>
			-->
			
		<core:Fragment id="formfragment" fragmentName="com.pe.pm.curd.pm_curd.view.FormEntity" type="XML" />
		

		</semantic:content>
		<semantic:editAction>
			<semantic:EditAction id="edit" press="onEdit"/>
		</semantic:editAction>
		<semantic:customFooterContent>
			<Button enabled="true" id="deleteButton" press="onDelete" text="Delete"/>
		</semantic:customFooterContent>
		<semantic:sendEmailAction>
			<semantic:SendEmailAction id="shareEmail" press="onShareEmailPress"/>
		</semantic:sendEmailAction>
		<semantic:shareInJamAction>
			<semantic:ShareInJamAction id="shareInJam" visible="{FLP>/isShareInJamActive}" press="onShareInJamPress"/>
		</semantic:shareInJamAction>
		<semantic:saveAsTileAction>
			<footerbar:AddBookmarkButton id="shareTile" title="{detailView>/saveAsTileTitle}"/>
		</semantic:saveAsTileAction>
	</semantic:DetailPage>
</mvc:View>